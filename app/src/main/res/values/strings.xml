    <!-- University Club Info-->

    <string name="university_club_summary_text">The University Club is open to members and
        non-members alike and serves a daily lunch buffet including a hot entree,
        salad bar, soup, and beverages.  Dessert is also available for a nominal extra
        charge. To learn about membership and reservations, please visit our location.
        </string>
    <string name="university_club_hours">
        Monday - Friday:     11:30AM - 2:00PM\n
        Saturday - Sunday:   Closed</string>
    <string name="university_club_location">
